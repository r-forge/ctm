> ## dgp
> dgp <- function(n = 200, seed = 25){
+         set.seed(seed)
+         x <- runif(n, 0, 20)
+         y <- as.integer(rnbinom(n, mu = exp(.2 + .1 * x), size = 3))
+         data.frame(x = x, y)
+ }
> 
> y <- 1:10
> m1 <- cotram(y ~ x, data = dgp())
