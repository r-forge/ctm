> GBSG2$y <- with(GBSG2, Surv(time, cens))
> 
> fm <- as.formula(paste("Surv(time, cens) ~ ", paste(xvar, collapse = "+")))
> cmod <- coxph(fm, data = GBSG2)
> 
> order <- 10
> by <- Bernstein_basis(numeric_var("y", support = c(0, max(GBSG2$time))), order = order,
